<h1>Timetrackingtypes</h1>
<table class="table table-striped">
  <thead>
    <tr>
      <th>ID</th>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @timetrackingtypes.each do |timetrackingtype| %>
      <tr>
        <td><%= timetrackingtype.id %></td>
        <td><%= link_to timetrackingtype.name, timetrackingtype_path(timetrackingtype) %></td>
        <td>
          <%= link_to 'Edit', edit_timetrackingtype_path(timetrackingtype), :class => 'btn btn-mini' %>
          <%= link_to 'Destroy', timetrackingtype_path(timetrackingtype), :method => :delete, :confirm => 'Are you sure?', :class => 'btn btn-mini btn-danger' %>
        </td>
      </tr>
    <% end %>
  </tbody>
</table>

<%= link_to 'New', new_timetrackingtype_path, :class => 'btn btn-primary' %>
